{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Details_cr</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Objet</th>
                <td>{{ entity.objet }}</td>
            </tr>
            <tr>
                <th>Datemotif</th>
                <td>{{ entity.dateMotif|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('details_cr') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('details_cr_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
