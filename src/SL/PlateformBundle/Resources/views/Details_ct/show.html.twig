{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Details_ct</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Nombreheure</th>
                <td>{{ entity.nombreHeure }}</td>
            </tr>
            <tr>
                <th>Dateenregistrement</th>
                <td>{{ entity.dateEnregistrement|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('details_ct') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('details_ct_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
